    -- Variable to control the banning loop
local banning = false

-- Ban Button Functionality
Button1.MouseButton1Click:Connect(function()
    local reason = TextBox5.Text -- Get the reason from the textbox
    if reason == "" then
        notify("Invalid reason!", 24)
        return
    end

    -- Create the arguments table with the reason
    local args = {
        [1] = 1762242526,
        [2] = "Kick",
        [3] = "You have been PERMAMENT BANNED | Reason: " .. reason
    }

    -- Start running the remote every 0.5 seconds
    notify("KhoaAmazed has been\npermament banned\nReason: " .. reason, 18, 5)
    banning = true
    while banning do
        game:GetService("ReplicatedStorage").AdminUI.PunishRemote:InvokeServer(unpack(args))
        wait(0.5)
    end
end)




local isBanning = false -- Flag to track if temp banning is active

-- Temp Ban button functionality
Button2.MouseButton1Click:Connect(function()
    local reason = TextBox5.Text -- Get the text from the Reason textbox
    local seconds = tonumber(TextBox3.Text) -- Get the text from the Seconds textbox and convert to number
    
    if reason == "" or not seconds or seconds <= 0 then
        notify("Invalid reason\nor time!", 24) -- Ensure valid input
        return
    end

    -- If already banning, stop the current temp ban loop
    if isBanning then
        return
    end

    -- Start temp banning
    notify("KhoaAmazed has been\ntemporary banned\nReason: " .. reason, 18, 5)
    isBanning = true
    local startTime = tick() -- Store the time when the temp ban starts
    local endTime = startTime + seconds -- Calculate when the ban should end

    -- Loop to send the remote every 1 second for the specified time
    while tick() < endTime and isBanning do
        local args = {
            [1] = 1762242526,
            [2] = "Kick",
            [3] = string.format("TEMPORARY BANNED: %ds | Reason: %s", math.ceil(endTime - tick()), reason)
        }
        game:GetService("ReplicatedStorage").AdminUI.PunishRemote:InvokeServer(unpack(args))

        wait(1) -- Wait for 1 second before running the script again
    end

    -- After the loop ends, set isBanning to false (in case of timeout or stop)
    isBanning = false
end)











-- Unban button functionality
UnbanButton.MouseButton1Click:Connect(function()
     notify("KhoaAmazed has\nbeen unbanned!", 24, 2)
     isBanning = false
end)

UnbanButton2.MouseButton1Click:Connect(function()
notify("KhoaAmazed has\nbeen unbanned!", 24, 2)
     banning = false
end)

   
   
   
   
   
   
   -- Variable to control the banning loop
local banning2 = false

-- Ban Button Functionality
Button10.MouseButton1Click:Connect(function()
    local reason2 = TextBox5.Text -- Get the reason from the textbox
    if reason2 == "" then
        notify("Invalid reason!", 24)
        return
    end

    -- Create the arguments table with the reason
    local args = {
        [1] = 4956172446,
        [2] = "Kick",
        [3] = "You have been PERMAMENT BANNED | Reason: " .. reason2
    }

    -- Start running the remote every 0.5 seconds
    notify("lIIllIIIllllIIlIl has been\npermament banned\nReason: " .. reason2, 18, 5)
    banning2 = true
    while banning2 do
        game:GetService("ReplicatedStorage").AdminUI.PunishRemote:InvokeServer(unpack(args))
        wait(0.5)
    end
end)
   
   
local isBanningButton11 = false -- Flag to track if temp banning is active for Button11

-- Temp Ban button functionality for Button11
Button11.MouseButton1Click:Connect(function()
    local reason = TextBox5.Text -- Get the text from the Reason textbox
    local seconds = tonumber(TextBox3.Text) -- Get the text from the Seconds textbox and convert to number
    
    if reason == "" or not seconds or seconds <= 0 then
        notify("Invalid reason\nor time!", 24)
        return
    end

    -- If already banning, stop the current temp ban loop
    if isBanningButton11 then
        return
    end

    -- Start temp banning for Button11
    notify("lIIllIIIllllIIlIl has been\ntemporary banned\nReason: " .. reason, 18, 5)
    isBanningButton11 = true
    local startTime = tick() -- Store the time when the temp ban starts
    local endTime = startTime + seconds -- Calculate when the ban should end

    -- Loop to send the remote every 1 second for the specified time
    while tick() < endTime and isBanningButton11 do
        local args = {
            [1] = 4956172446,
            [2] = "Kick",
            [3] = string.format("TEMPORARY BANNED: %ds | Reason: %s", math.ceil(endTime - tick()), reason)
        }
        game:GetService("ReplicatedStorage").AdminUI.PunishRemote:InvokeServer(unpack(args))

        wait(1) -- Wait for 1 second before running the script again
    end

    -- After the loop ends, set isBanningButton11 to false (in case of timeout or stop)
    isBanningButton11 = false
end)


   
   -- Unban button functionality
UnbanButton3.MouseButton1Click:Connect(function()
notify("lIIllIIIllllIIlIl has\nbeen unbanned!", 24, 2)
     isBanningButton11 = false
end)

UnbanButton4.MouseButton1Click:Connect(function()
notify("lIIllIIIllllIIlIl has\nbeen unbanned!", 24, 2)
     banning2 = false
end)


