Button23.MouseButton1Click:Connect(function()
    local inputName = TextBox1.Text
    local playerName = completePlayerName(inputName)

    if playerName then
        local engineerBuilds = workspace:FindFirstChild("Engineer Builds")
        if engineerBuilds then
            local engineerBuild = engineerBuilds:FindFirstChild(playerName)
            if engineerBuild then
                local remoteEvents = game:GetService("ReplicatedStorage"):FindFirstChild("Remote Events")
                local punchEvent = remoteEvents and remoteEvents:FindFirstChild("Punch")
                
                if not punchEvent then
                    print("Remote event 'Punch' not found!")
                    return
                end

                for _, tree in pairs(engineerBuild:GetChildren()) do
                    local validObjects = {
                        "Dispenser", "Teleporter", "Driver's Seat", "Floor", "Tree", "Radio", "Launch Pad",
                        "Wall", "Wheel", "Balloon", "Seat", "Ladder", "Piston", "Overclocker", "Propeller", "Fan"
                    }

                    if table.find(validObjects, tree.Name) then
                        local targetPart = tree:FindFirstChild("HumanoidRootPart") or tree.PrimaryPart
                        if targetPart then
                            local position = Vector3.new(61.8017, 19.4766, -42.9627) -- Default position
                            local force = 1.9991 -- Default force
                            
                            if tree.Name == "Driver's Seat" then
                                position = Vector3.new(-61.3097, 28.1996, -47.5602)
                                force = 2.2868
                            elseif tree.Name == "Launch Pad" then
                                position = Vector3.new(-9.8040, 19.1488, 39.6843)
                                force = 1.6987
                            end

                            for i = 1, 15 do
                                punchEvent:FireServer(8923749671123, tree, position, force, targetPart)
                            end
                        end
                    end
                end
            else
                TextBox1.Text = "Build not found!"
                wait(2)
                TextBox1.Text = ""
                TextBox1.PlaceholderText = "Enter Player Name"
            end
        else
            TextBox1.Text = "Engineer Builds not found!"
            wait(2)
            TextBox1.Text = ""
            TextBox1.PlaceholderText = "Enter Player Name"
        end
    else
        TextBox1.Text = "Player not found!"
        wait(2)
        TextBox1.Text = ""
        TextBox1.PlaceholderText = "Enter Player Name"
    end
end)


local looping = false -- Toggle state

Button24.MouseButton1Click:Connect(function()
    looping = not looping -- Toggle state

    -- Update button color (Green = ON, Red = OFF)
    Button24.TextColor3 = looping and Color3.new(0, 1, 0) or Color3.new(1, 0, 0)

    if looping then
        spawn(function() -- Run in a separate thread
            while looping do
                local inputName = TextBox1.Text
                local playerName = completePlayerName(inputName)

                if playerName then
                    local engineerBuilds = workspace:FindFirstChild("Engineer Builds")
                    if engineerBuilds then
                        local engineerBuild = engineerBuilds:FindFirstChild(playerName)
                        if engineerBuild then
                            local remoteEvents = game:GetService("ReplicatedStorage"):FindFirstChild("Remote Events")
                            local punchEvent = remoteEvents and remoteEvents:FindFirstChild("Punch")
                            
                            if not punchEvent then
                                print("Remote event 'Punch' not found!")
                                return
                            end

                            local validObjects = {
                                "Dispenser", "Teleporter", "Driver's Seat", "Floor", "Tree", "Radio", "Launch Pad",
                                "Wall", "Wheel", "Balloon", "Seat", "Ladder", "Piston", "Overclocker", "Propeller", "Fan"
                            }

                            for _, tree in pairs(engineerBuild:GetChildren()) do
                                if not looping then break end -- Stop immediately if toggled off

                                if table.find(validObjects, tree.Name) then
                                    local targetPart = tree:FindFirstChild("HumanoidRootPart") or tree.PrimaryPart
                                    if targetPart then
                                        local position = Vector3.new(61.8017, 19.4766, -42.9627) -- Default position
                                        local force = 1.9991 -- Default force
                                        
                                        if tree.Name == "Driver's Seat" then
                                            position = Vector3.new(-61.3097, 28.1996, -47.5602)
                                            force = 2.2868
                                        elseif tree.Name == "Launch Pad" then
                                            position = Vector3.new(-9.8040, 19.1488, 39.6843)
                                            force = 1.6987
                                        end

                                        for i = 1, 15 do
                                            if not looping then break end -- Stop mid-execution if toggled off
                                            punchEvent:FireServer(8923749671123, tree, position, force, targetPart)
                                        end
                                    end
                                end
                            end
                        else
                            TextBox1.Text = "Build not found!"
                            task.wait(2)
                            TextBox1.Text = ""
                            TextBox1.PlaceholderText = "Enter Player Name"
                        end
                    else
                        TextBox1.Text = "Engineer Builds not found!"
                        task.wait(2)
                        TextBox1.Text = ""
                        TextBox1.PlaceholderText = "Enter Player Name"
                    end
                else
                    TextBox1.Text = "Player not found!"
                    task.wait(2)
                    TextBox1.Text = ""
                    TextBox1.PlaceholderText = "Enter Player Name"
                end

                task.wait(0.1) -- **Prevents game from crashing**
            end
        end)
    end
end)


local loopActive = false

Button27.MouseButton1Click:Connect(function()
    loopActive = not loopActive
    Button27.TextColor3 = loopActive and Color3.new(0,1,0) or Color3.new(1,0,0) -- Green when on, red when off

    task.spawn(function()
        while loopActive do
            local inputName = TextBox1.Text
            local playerName = completePlayerName(inputName)

            if playerName then
                local engineerBuild = workspace:FindFirstChild("Engineer Builds"):FindFirstChild(playerName)
                if engineerBuild then
                    for _, tree in pairs(engineerBuild:GetChildren()) do
                        if tree.Name == "Floor" then
                            for i = 1, 15 do
                                if not loopActive then return end -- Stop if toggled off

                                task.defer(function()
                                    local args = {
                                        [1] = 8923749671123,
                                        [2] = tree,
                                        [3] = Vector3.new(61.80173873901367, 19.47662925720215, -42.96271896362305),
                                        [4] = 1.999159812927246,
                                        [5] = tree.HumanoidRootPart
                                    }

                                    game:GetService("ReplicatedStorage"):FindFirstChild("Remote Events").Punch:FireServer(unpack(args))
                                end)
                            end
                        end
                    end
                else
                    TextBox1.Text = "Build not found!"
                    task.wait(2)
                    TextBox1.Text = ""
                    TextBox1.PlaceholderText = "Enter Player Name"
                end
            else
                TextBox1.Text = "Player not found!"
                task.wait(2)
                TextBox1.Text = ""
                TextBox1.PlaceholderText = "Enter Player Name"
            end

            task.wait() -- Prevents infinite freeze but keeps it fast
        end
    end)
end)


