---------TEXTBOX---------


-- TextBox

TextBox1.Parent = Frame1
TextBox1.Size = UDim2.new(0, 150, 0, 50)
TextBox1.Position = UDim2.new(0.5, -150, 0.25, -20)
TextBox1.BackgroundColor3 = Color3.new(0, 0, 0)
TextBox1.BackgroundTransparency = 0.5
TextBox1.Text = "Enter Player Name"
TextBox1.TextSize = 18
TextBox1.Font = Enum.Font.FredokaOne
TextBox1.TextColor3 = Color3.new(1, 1, 1) 

TextBox1.PlaceholderText = "Enter Player Name"
TextBox1.PlaceholderColor3 = Color3.new(0.7, 0.7, 0.7)
TextBox1.ClearTextOnFocus = true

-- Corner radius for TextBox

TextBoxUICorner.Parent = TextBox1
TextBoxUICorner.CornerRadius = UDim.new(0, 12)

-- Black outline for TextBox

TextBoxStroke.Parent = TextBox1
TextBoxStroke.Color = Color3.new(0,0,0)
TextBoxStroke.Thickness = 2


local function completePlayerName(inputName)
    local players = game:GetService("Players")
    for _, player in ipairs(players:GetPlayers()) do
        if string.lower(player.Name):sub(1, #inputName) == string.lower(inputName) or 
           string.lower(player.DisplayName):sub(1, #inputName) == string.lower(inputName) then
            return player.Name -- Return username if match is found
        end
    end
    return nil -- If no match is found
end


TextBox1.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        local inputName = TextBox1.Text
        if inputName == "" then
            TextBox1.PlaceholderText = "Enter Player Name"
        else
            local playerName = completePlayerName(inputName)
            if playerName then
                TextBox1.Text = playerName
            else
                TextBox1.Text = "Player not found"
                wait(2)
                TextBox1.Text = "Enter Player Name"
            end
        end
    end
end)


Button4.MouseButton1Click:Connect(function()
    local inputName = TextBox1.Text
    local playerName = completePlayerName(inputName)

    if playerName then
        local engineerBuild = workspace:FindFirstChild("Engineer Builds"):FindFirstChild(playerName)
        if engineerBuild then
            for _, tree in pairs(engineerBuild:GetChildren()) do
                if tree.Name == "Tree" then
                    for i = 1, 15 do
                        local args = {
                            [1] = 8923749671123,
                            [2] = tree,
                            [3] = Vector3.new(61.80173873901367, 19.47662925720215, -42.96271896362305),
                            [4] = 1.999159812927246,
                            [5] = tree.HumanoidRootPart
                        }

                        game:GetService("ReplicatedStorage"):FindFirstChild("Remote Events").Punch:FireServer(unpack(args))
                    end
                end
            end
        else
            TextBox1.Text = "Build not found!"
            wait(2)
            TextBox1.Text = ""
            TextBox1.PlaceholderText = "Enter Player Name"
        end
    else
        TextBox1.Text = "Player not found!"
        wait(2)
        TextBox1.Text = ""
        TextBox1.PlaceholderText = "Enter Player Name"
    end
end)





