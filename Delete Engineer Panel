UIStroke4.ApplyStrokeMode = Enum.ApplyStrokeMode.Border

-- Click event for Button1
Button1.MouseButton1Click:Connect(function()

local guiName = "EZLOLKID"

local coreGui = game.CoreGui
local deletedCount = 0

for _, child in pairs(coreGui:GetChildren()) do
    if child.Name == guiName then
        child:Destroy()
        deletedCount += 1
    end
end

if deletedCount > 0 then
    print(deletedCount .. " GUI(s) named " .. guiName .. " have been deleted.")
else
    print("No GUI named " .. guiName .. " found.")
end


end)



Button2.Parent = Frame1
Button2.Size = UDim2.new(0, 150, 0, 40)
Button2.Position = UDim2.new(0.5, -150, 0.61, -20)
Button2.BackgroundColor3 = Color3.new(0, 0, 0)
Button2.BackgroundTransparency = 0.7
Button2.Text = "Tp To Safespot"
Button2.TextSize = 18
Button2.Font = Enum.Font.FredokaOne
Button2.TextColor3 = Color3.new(1, 1, 1)
Button2.TextStrokeTransparency = 1

-- Black outline for Button2
local UIStroke3 = Instance.new("UIStroke", Button2)
UIStroke3.Color = Color3.new(0, 0, 0)
UIStroke3.Thickness = 2

-- Corner radius for Button2
local UICorner2 = Instance.new("UICorner") 
UICorner2.Parent = Button2
UICorner2.CornerRadius = UDim.new(0, 12)

-- Add outline to Button2
local UIStroke4 = Instance.new("UIStroke")
UIStroke4.Parent = Button2
UIStroke4.Thickness = 2
UIStroke4.Color = Color3.new(1, 1, 1)
UIStroke4.ApplyStrokeMode = Enum.ApplyStrokeMode.Border



local Players = game:GetService("Players")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local originalPosition = nil
local createdPart = nil

Button2.Text = "Tp To Safespot"

-- Function to teleport the player
local function teleportPlayer(position)
    if character and character:FindFirstChild("HumanoidRootPart") then
        character.HumanoidRootPart.CFrame = CFrame.new(position)
    end
end

-- Update character reference after resetting
local function updateCharacter(newCharacter)
    character = newCharacter
end

player.CharacterAdded:Connect(updateCharacter)

-- Button click functionality
Button2.MouseButton1Click:Connect(function()
    if Button2.Text == "Tp To Safespot" then
        if character and character:FindFirstChild("HumanoidRootPart") then
            originalPosition = character.HumanoidRootPart.Position
        end


        createdPart = Instance.new("Part")
        createdPart.Position = Vector3.new(-4740.06, 100000, -4743.73)
        createdPart.Size = Vector3.new(2048, 1, 2048)
        createdPart.Anchored = true
        createdPart.Material = Enum.Material.Plastic -- Optional: Set material
        createdPart.Transparency = 0.5
        createdPart.Parent = workspace

        teleportPlayer(Vector3.new(-4739.6591796875, 100003.5, -4744.0185546875))

        Button2.Text = "Tp Back"
    elseif Button2.Text == "Tp Back" then
        if createdPart then
            createdPart:Destroy()
            createdPart = nil
        end

        if originalPosition then
            teleportPlayer(originalPosition)
        end

        Button2.Text = "Tp To Safespot"
    end
end)



