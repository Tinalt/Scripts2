local Tab1Section = Tab1:NewSection("Main")

local Tab2 = window:NewTab("Misc")
local Tab2Section = Tab2:NewSection("Misc")

local Tab3 = window:NewTab("Credits")
local Tab3Section = Tab3:NewSection("Credits")

